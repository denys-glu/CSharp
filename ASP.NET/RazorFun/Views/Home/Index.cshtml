@{
    ViewData["Title"] = "Home Page";
}

@{
    var StringList = new List<string>
    {
        "Apple Pie",
        "Burritos",
        "Clams Casino",
        "Donuts"
     };
        
    foreach(var word in StringList)
    {
        // We can render HTML from within C# code!
        @if(word.StartsWith("C"))
        {
            <p style="color: red;">@word</p>
        }
        else 
        {
            <p>@word</p>
        }
    }
}
